<Page
    x:Class="Jaar_1_Project_4.Wijnhaven99"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Jaar_1_Project_4"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{StaticResource hrTheme}" x:Name="wijnhaven_99">
        <TextBlock x:Name="mainTitle" HorizontalAlignment="Left" Margin="140,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Bold" Foreground="#FFFFF8F8">
            <Run Text="Wijnhaven 99"/>
            <LineBreak/>
            <Run/>
        </TextBlock>
        <Button x:Name="educationButton" Content="Education" HorizontalAlignment="Left" Margin="140,45,0,0" VerticalAlignment="Top" Click="educationButton_Click"/>
        <Button x:Name="backButton" Content="Back" HorizontalAlignment="Left" Margin="154,160,0,0" VerticalAlignment="Top" Click="backButton_Click"/>
        <Button x:Name="info" Content="Info" HorizontalAlignment="Left" Margin="154,100,0,0" VerticalAlignment="Top" Click="info_Click"/>

    </Grid>
</Page>
