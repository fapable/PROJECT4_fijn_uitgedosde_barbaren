<Application
    x:Class="Jaar_1_Project_4.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Jaar_1_Project_4"
    RequestedTheme="Light">
    <Application.Resources>
        <SolidColorBrush x:Key="hrTheme" Color="#FFCF0044"/>
        <Style x:Key="mainTitle" TargetType="TextBlock"/>
        <Style TargetType="TextBlock"/>
        <Style x:Key="toiletColour" TargetType="Border"/>
        <Style x:Key="classroomColour" TargetType="Border"/>
        <Style x:Key="projectSpaceColour" TargetType="Border"/>
        <FontFamily x:Key="ContentControlThemeFontFamily">Consolas</FontFamily>
    </Application.Resources>

</Application>

