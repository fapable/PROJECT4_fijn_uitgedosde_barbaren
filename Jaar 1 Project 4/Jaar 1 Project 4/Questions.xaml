<Page
    x:Class="Jaar_1_Project_4.Questions"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Jaar_1_Project_4"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{StaticResource hrTheme}" x:Name="questions">
        <TextBlock x:Name="mainTitle" HorizontalAlignment="Left" Margin="165,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Bold" Foreground="#FFFFF8F8" SelectionChanged="mainTitle_SelectionChanged">
            <Run Text="Questions
                 "/>
            <LineBreak/>
            <Run/>
        </TextBlock>
        <Button x:Name="back" Content="Back" HorizontalAlignment="Left" Margin="165,170,0,0" VerticalAlignment="Top" Click="back_Click"/>

    </Grid>
</Page>
