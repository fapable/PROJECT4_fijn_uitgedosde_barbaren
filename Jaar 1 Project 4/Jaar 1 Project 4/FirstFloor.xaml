<Page
    x:Class="Jaar_1_Project_4.FirstFloor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Jaar_1_Project_4"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{StaticResource hrTheme}">
        <TextBlock x:Name="mainTitle" HorizontalAlignment="Left" Margin="140,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Bold" Foreground="#FFFFF8F8">
            <Run Text="First floor"/>
            <LineBreak/>
            <Run/>
        </TextBlock>
        <Button x:Name="firstFlourBackButton" Content="Back" HorizontalAlignment="Left" Margin="155,158,0,0" VerticalAlignment="Top" Click="firstFlourBackButton_Click"/>

    </Grid>
</Page>
